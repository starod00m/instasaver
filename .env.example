# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Proxy Configuration (optional, required for TikTok downloads)
# Format: http://username:password@host:port or socks5://username:password@host:port
PROXY_URL=

# Admin Configuration
# Telegram user ID of the bot administrator (can view statistics)
ADMIN_USER_ID=123456789

# Google Sheets Statistics (optional)
# Service account credentials JSON as a single line string
# Example: {"type":"service_account","project_id":"your-project","private_key":"...","client_email":"..."}
GOOGLE_CREDENTIALS_JSON=
# Google Sheets spreadsheet ID (from the URL between /d/ and /edit)
GOOGLE_SHEETS_SPREADSHEET_ID=
